    "singularName": "navigation-item",
    "pluralName": "navigation-items",
    "displayName": "NavigationItems"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "navigation_item": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::navigation-item.navigation-item",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            false
          ]
        }
      },
      "inversedBy": "navigation_items"
    },
    "navigation_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::navigation-item.navigation-item",
      "mappedBy": "navigation_item"
    },
    "title": {
      "type": "string"
    },
    "isLogo": {
      "type": "boolean"
    },
    "order": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            false
          ]
        }
      }
    },
    "url": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            false
          ]
        }
      }
    },
    "visible": {
      "type": "boolean",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            false
          ]
        }
      }
    },
    "isChild": {
      "type": "boolean",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            false
          ]
        }
      }
    },
    "logoUrl": {
      "type": "media",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            true
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "footer": {
      "type": "blocks",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            true
          ]
        }
      }
    },
    "phone": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isLogo"
            },
            true
          ]
        }
      }
