{
  "kind": "collectionType",
  "collectionName": "artikels",
  "info": {
    "singularName": "artikel",
    "pluralName": "artikels",
    "displayName": "Artikel"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "category": {
      "type": "string"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "string"
    },
    "metaImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "metaKeywords": {
      "type": "string"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared.content-block",
        "shared.gallery-block"
      ]
    },
    "gallery_items": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::gallery-item.gallery-item",
      "inversedBy": "artikels"
    }
  }
}
